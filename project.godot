; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[YarnSpinnerGodot]

YarnProjectPaths=["res://VN/Yarn/Dialogue.tres"]

[application]

config/name="MultiWindowVN"
run/main_scene="res://Title Screen/TitleScene.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
run/max_fps=165
config/icon="res://icon.svg"

[display]

window/size/mode=3
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0
window/ios/allow_high_refresh_rate=false

[dotnet]

project/assembly_name="MultiWindowVN"

[editor_plugins]

enabled=PackedStringArray("res://addons/YarnSpinner-Godot/plugin.cfg")

[importer_defaults]

animation_library={
"animation/fps": 60.0
}

[rendering]

renderer/rendering_method="mobile"
